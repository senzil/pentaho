{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "db-data",
      "host": {
        "sourcePath": "/var/lib/postgresql/data"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "pentaho",
      "image": "senzil/pentaho:6.1",
      "essential": true,
      "memory": 2048,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ]
    }
  ]
}